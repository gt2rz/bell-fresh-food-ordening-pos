---
let { height, width, fill } = Astro.props;
width = width ? width : "24";
height = height ? height : "24";
fill = fill ? fill : "currentColor";
---

<!--?xml version="1.0" encoding="UTF-8"?-->
<svg
  xmlns="http://www.w3.org/2000/svg"
  id="Outline"
  viewBox="0 0 24 24"
  width= { width }
  height= { height }
  fill= { fill }
>
  <rect y="11" width="24" height="2" rx="1"></rect><rect
    y="4"
    width="24"
    height="2"
    rx="1"></rect>
  <rect y="18" width="24" height="2" rx="1"></rect>
</svg>
