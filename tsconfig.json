{
  "extends": "astro/tsconfigs/strict",
  "compilerOptions": {
    "baseUrl": ".",
    "paths": {
      "@/*": [
        "src/*"
      ],
      "@/components/*": [
        "src/components/*"
      ],
      "@/layouts/*": [
        "src/layouts/*"
      ],
      "@/icons/*": [
        "src/icons/*"
      ],
      "@/icons/categories/*": [
        "src/icons/categories/*"
      ],
      "@/data/*": [
        "src/data/*"
      ],
    }
  }
}